{
  "name": "ugross.dev-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run codegen",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js & npm run lint:css",
    "lint:js": "next lint",
    "lint:css": "stylelint **/*.css",
    "husky:precommit": "lint-staged",
    "codegen:ci": "graphql-codegen --config codegen.yml",
    "codegen": "DOTENV_CONFIG_PATH=.env.local graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "src/**.{js,jsx,tsx,ts,scss,css}": "prettier --write",
    "src/**.{js,jsx,ts,tsx}": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.2",
    "@mdx-js/loader": "^2.1.1",
    "@mdx-js/mdx": "^2.1.1",
    "@mdx-js/react": "^2.1.1",
    "@next/mdx": "^12.1.6",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "feed": "^4.2.2",
    "graphql": "^16.5.0",
    "graphql-request": "^4.2.0",
    "match-sorter": "^6.3.1",
    "next": "12.1.6",
    "next-mdx-remote": "^4.0.3",
    "next-pwa": "^5.5.2",
    "prism-react-renderer": "^1.3.3",
    "prismjs": "^1.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.3.1",
    "react-use": "^17.4.0",
    "rehype-external-links": "^1.0.1",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-prism": "^1.3.6",
    "remark-toc": "^8.0.1",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.1.1",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.4",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  }
}
