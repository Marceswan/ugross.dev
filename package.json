{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "1.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "tsc --noEmit --skipLibCheck && eslint 'src/*/**.{js,ts,tsx}' --quiet --fix",
    "lint:md": "remark ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "gatsby": "^2.13.76",
    "gatsby-image": "^2.2.12",
    "gatsby-plugin-google-analytics": "^2.1.9",
    "gatsby-plugin-manifest": "^2.2.8",
    "gatsby-plugin-netlify-cms": "^4.1.9",
    "gatsby-plugin-offline": "^2.2.9",
    "gatsby-plugin-react-helmet": "^3.1.4",
    "gatsby-plugin-sharp": "^2.2.16",
    "gatsby-plugin-typescript": "^2.1.4",
    "gatsby-remark-autolink-headers": "^2.1.6",
    "gatsby-remark-copy-linked-files": "^2.1.9",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.16",
    "gatsby-remark-prismjs": "^3.3.8",
    "gatsby-remark-responsive-iframe": "^2.2.7",
    "gatsby-source-filesystem": "^2.1.14",
    "gatsby-transformer-remark": "^2.6.18",
    "gatsby-transformer-sharp": "^2.2.10",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.9.7",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-protected-mailto": "^1.0.3",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "gatsby-plugin-sass": "^2.1.11",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-stylelint": "^3.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "typescript": "^3.5.3"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "no-descending-specificity": null
    }
  }
}
