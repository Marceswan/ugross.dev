{
  "name": "ugross.dev-next",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "predev": "npm run codegen",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "npm run lint:js & npm run lint:css",
    "lint:js": "next lint",
    "lint:css": "stylelint src/**/*.css",
    "husky:precommit": "lint-staged",
    "codegen": "DOTENV_CONFIG_PATH=.env.local graphql-codegen --config codegen.yml -r dotenv/config",
    "prepare": "husky install",
    "postinstall": "npm run codegen"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "src/**.{js,jsx,tsx,ts,scss,css}": "prettier --write",
    "src/**.{js,jsx,ts,tsx}": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@mdx-js/loader": "^2.1.2",
    "@mdx-js/mdx": "^2.1.2",
    "@mdx-js/react": "^2.1.2",
    "@next/mdx": "^12.2.5",
    "@shopify/browserslist-config": "^3.0.0",
    "@shopify/polaris-tokens": "^5.5.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.5",
    "feed": "^4.2.2",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "next": "12.2.5",
    "next-mdx-remote": "^4.1.0",
    "next-pwa": "^5.5.5",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.4.0",
    "react-use": "^17.4.0",
    "rehype-external-links": "^1.0.1",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-prism": "^1.3.6",
    "remark-toc": "^8.0.1",
    "styled-components": "^5.3.5",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
