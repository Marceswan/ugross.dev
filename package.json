{
  "name": "ugross-dev",
  "private": true,
  "description": "Personal site",
  "version": "1.0.0",
  "author": "Rostyslav Ugryniuk <ug.ross@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "tslint --project 'tsconfig.json'",
    "lint:md": "remark ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/ugross.dev"
  },
  "dependencies": {
    "gatsby": "^2.3.14",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-typescript": "^2.0.12",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.7",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "polished": "^3.2.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-protected-mailto": "^1.0.3",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.8",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.13",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-stylelint": "^3.2.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "no-descending-specificity": null
    }
  }
}
